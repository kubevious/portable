{
  "name": "kubevious-portable",
  "version": "1.0.1",
  "description": "Kubevious Portable Backend",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.ts",
    "server": "node dist/index.js",
    "client": "cd ../client && npm start",
    "client-mock": "npm run start-mock --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-mock": "concurrently \"npm run client-mock\"",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://kubevious@github.com/kubevious/portable.git"
  },
  "dependencies": {
    "@kubevious/helper-backend": "^1.0.17",
    "@kubevious/helper-logic-processor": "^1.0.9",
    "@kubevious/helpers": "^1.0.49",
    "@kubevious/websocket-server": "^1.0.13",
    "@kubevious/worldvious-client": "^1.0.6",
    "base-64": "^0.1.0",
    "cron": "^1.8.2",
    "express": "^4.18.2",
    "flexsearch": "^0.6.32",
    "joi": "^17.3.0",
    "js-yaml": "^3.14.1",
    "k8s-super-client": "^2.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "the-lodash": "^2.0.0",
    "the-logger": "^2.0.5",
    "the-promise": "^2.0.4",
    "uuid": "^7.0.2",
    "uuid-parse": "^1.1.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/base-64": "^0.1.3",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.5",
    "@types/should": "^13.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "mocha": "^8.1.1",
    "prettier": "^2.1.0",
    "should": "^13.2.3",
    "typescript": "^4.1.3"
  }
}
